{
	"extends": "@tsconfig/svelte/tsconfig.json",
	"compilerOptions": {
		"lib": [
			"DOM",
			"ES2017",
			"WebWorker",
			"ESNext"
		],
		"strict": true,
		"noImplicitAny": true,
		// "module": "UMD", /* Specify moduleC code generation: 'none', 'commonjs', 'amd', 'system', 'umd', 'es2015', or 'ESNext'. */
		"target": "ESNext"
	},
	"include": [
		"src/**/*",
	],
	"exclude": [
		"node_modules/*",
		"__sapper__/*",
		"static/*"
	]
}